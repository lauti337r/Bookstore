{% extends 'base.html.twig' %}

{% block title %}Hello {% endblock %}

{% block body %}
    <style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

    <div class="row">
        <div class="col-9 bg-danger">{% block left %}{% endblock left %}</div>
        <div class="col-3 bg-warning">{% if not app.user %}<a href="{{ path('connect_google') }}"><img width="150" class="img-fluid mx-auto d-block" src="{{ asset('build/images/btn_google.png') }}"></a>{% endif %} </div>
    </div>


{% endblock %}
